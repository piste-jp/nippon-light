			"scope": [
				"markup.output",
				"markup.raw"
			],
			"settings": {
				"foreground": "#544a47"  // 黒橡 くろつるばみ
			}
		},
		{
			"name": "Markup: Prompt",
			"scope": "markup.prompt",
			"settings": {
				"foreground": "#544a47"  // 黒橡 くろつるばみ
			}
